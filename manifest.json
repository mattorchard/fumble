{
  "name": "Fumble",
  "short_name": "Fumble",
  "version": "0.0.1",
  "description": "Saves your work so you don't have to",
  "icons": {},
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
